<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qa.po.EtcCard">
    <select id="findUserById" parameterType="String" resultType="com.qa.po.EtcCard">
        select * from etc_card where id = #{id}
    </select>

    <select id="findUserByList" parameterType="String" resultType="com.qa.po.EtcCard">
        select * from etc_card where vehicle_code like '${value}%'
    </select>

</mapper>